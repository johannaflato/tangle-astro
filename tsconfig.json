{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    // Alias `src` to `~`
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    }
  }
}